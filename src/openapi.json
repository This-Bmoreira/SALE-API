{
    "openapi": "3.0.0",
    "info": {
        "title": "User API",
        "version": "1.0.0"
    },
    "paths": {
        "/user": {
            "post": {
                "summary": "Create a new user",
                "tags": [
                    "user"
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Bruno Moreira"
                                    },
                                    "password": {
                                        "type": "string",
                                        "example": "111111"
                                    },
                                    "email": {
                                        "type": "string",
                                        "example": "bm@gmail.com"
                                    },
                                    "phone": {
                                        "type": "string",
                                        "example": "90000-0000"
                                    },
                                    "cpf": {
                                        "type": "string",
                                        "example": "341.321.333-32"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "User created successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "name": {
                                            "type": "string",
                                            "example": "Bruno Moreira"
                                        },
                                        "password": {
                                            "type": "string",
                                            "example": "$2b$10$5UMVPcDoLzRT/udQs5JWl.dnN0HBdBbl6Z6Y/GV/R4Ic39paPBklS"
                                        },
                                        "email": {
                                            "type": "string",
                                            "example": "bm@gmail.com"
                                        },
                                        "phone": {
                                            "type": "string",
                                            "example": "90000-0000"
                                        },
                                        "cpf": {
                                            "type": "string",
                                            "example": "341.321.333-32"
                                        },
                                        "typeUser": {
                                            "type": "integer",
                                            "example": 1
                                        },
                                        "id": {
                                            "type": "integer",
                                            "example": 2
                                        },
                                        "created_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2023-07-05T16:07:30.531Z"
                                        },
                                        "updated_at": {
                                            "type": "string",
                                            "format": "date-time",
                                            "example": "2023-07-05T16:07:30.531Z"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "get": {
                "summary": "Get all users",
                "tags": [
                    "user"
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer"
                                            },
                                            "name": {
                                                "type": "string"
                                            },
                                            "email": {
                                                "type": "string"
                                            },
                                            "cpf": {
                                                "type": "string"
                                            },
                                            "typeUser": {
                                                "type": "integer"
                                            },
                                            "phone": {
                                                "type": "string"
                                            },
                                            "password": {
                                                "type": "string"
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "format": "date-time"
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "format": "date-time"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/state": {
            "get": {
                "summary": "Get list of states",
                "tags": [
                    "state"
                ],
                "responses": {
                    "200": {
                        "description": "Sucesso",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "description": "ID do estado",
                                                "example": 19
                                            },
                                            "name": {
                                                "type": "string",
                                                "description": "Nome do estado",
                                                "example": "Rio de Janeiro"
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "format": "date-time",
                                                "description": "Data de criação do estado",
                                                "example": "2023-07-05T13:47:01.373Z"
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "format": "date-time",
                                                "description": "Data de atualização do estado",
                                                "example": "2023-07-05T13:47:01.373Z"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/city/{stateId}": {
            "parameters": [
                {
                    "name": "stateId",
                    "in": "path",
                    "description": "ID do estado",
                    "required": true,
                    "schema": {
                        "type": "integer"
                    },
                    "example": 19
                }
            ],
            "get": {
                "summary": "Get city information with specified state ID",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 3591
                                            },
                                            "stateId": {
                                                "type": "integer",
                                                "example": 19
                                            },
                                            "name": {
                                                "type": "string",
                                                "example": "Angra dos Reis"
                                            },
                                            "created_at": {
                                                "type": "string",
                                                "format": "date-time",
                                                "example": "2023-07-05T13:47:01.373Z"
                                            },
                                            "updated_at": {
                                                "type": "string",
                                                "format": "date-time",
                                                "example": "2023-07-05T13:47:01.373Z"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}